IF
	Global("TIBLARS","GLOBAL",0)
	Gender([INANIMATE.HUMANOID],MALE)
THEN
	RESPONSE #100
		SetGlobal("TIBLARS","GLOBAL",1)
		Polymorph(FIGHTER_MALE_ELF)
		ChangeRace([INANIMATE.HUMANOID],ELF)
END

IF
	Global("TIBLARS","GLOBAL",0)
	Gender([INANIMATE.HUMANOID],FEMALE)
THEN
	RESPONSE #100
		SetGlobal("TIBLARS","GLOBAL",1)
		Polymorph(FIGHTER_FEMALE_ELF)
		ChangeRace([INANIMATE.HUMANOID],ELF)
END

